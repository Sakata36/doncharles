using System;

class Program
{
    static void Main()
    {
        int n = 10; // You can change this to set the range of the multiplication table

        for (int i = 1; i <= n; i++)
        {
            for (int j = 1; j <= n; j++)
            {
                Console.Write($"{i} x {j} = {i * j}\t");
            }
            Console.WriteLine();
        }
    }
}
